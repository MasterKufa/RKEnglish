{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "ducks/*": [
        "redux/*"
      ],
      "assets/*": [
        "assets/*"
      ],
      "app/*": [
        "app/*"
      ],
      "components/*": [
        "components/*"
      ],
      "globalStyle/*": [
        "globalStyle/*"
      ],
      "crypto": [
        "./node_modules/crypto-browserify"
      ]
    }
  }
}